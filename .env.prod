# Environment
ENVIRONMENT='prod'
DEBUG=False

# Database
MYSQL_DATABASE='your_prod_db'
MYSQL_USER='your_prod_user'
MYSQL_PASSWORD='your_secure_password'
MYSQL_HOST='db'
MYSQL_PORT='3306'
MYSQL_ROOT_PASSWORD='your_secure_root_password'

# SSL Certificates
MYSQL_SSL_CA='/etc/mysql/certs/ca.pem'
MYSQL_SSL_CERT='/etc/mysql/certs/client-cert.pem'
MYSQL_SSL_KEY='/etc/mysql/certs/client-key.pem'

# Discord
DISCORD_KEY='your_prod_discord_key'

# Django
SECRET_KEY='your_prod_secret_key'

# Simple JWT
SIGNING_KEY='your_prod_signing_key'

# Production Settings
ALLOWED_HOSTS='your-production-domain.com,web'
API_BASE_URL='https://your-production-domain.com'